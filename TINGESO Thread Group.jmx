            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import java.time.LocalDateTime
import java.time.format.DateTimeFormatter

// Obtener el valor del contador de forma segura
String counterValue = vars.get(&quot;request_counter&quot;);
int counter = 0; // Valor por defecto

// Verificar si la variable existe antes de usarla
if (counterValue != null &amp;&amp; !counterValue.isEmpty()) {
    counter = Integer.parseInt(counterValue);
} else {
    // Si la variable no existe, registra un error para saberlo
    log.error(&quot;La variable &apos;request_counter&apos; no fue encontrada. Usando valor por defecto 0.&quot;);
}

// --- El resto del script es igual ---

long startMinutesOffset = counter * 30;
long endMinutesOffset = startMinutesOffset + 30;

LocalDateTime now = LocalDateTime.now();
DateTimeFormatter formatter = DateTimeFormatter.ofPattern(&quot;yyyy-MM-dd&apos;T&apos;HH:mm:ss&quot;);

LocalDateTime inicioReservaDate = now.plusMinutes(startMinutesOffset);
LocalDateTime finReservaDate = now.plusMinutes(endMinutesOffset);

String inicioReservaStr = inicioReservaDate.format(formatter);
String finReservaStr = finReservaDate.format(formatter);

vars.put(&quot;inicio_reserva_final&quot;, inicioReservaStr);
vars.put(&quot;fin_reserva_final&quot;, finReservaStr);</stringProp>
            <stringProp name="scriptLanguage">groovy</stringProp>
